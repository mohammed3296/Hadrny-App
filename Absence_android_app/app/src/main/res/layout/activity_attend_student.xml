<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.work.unknown.absence.Tutor.AttendStudentActivity">

    <Spinner
        android:id="@+id/sessions_spinner"
        android:layout_width="334dp"
        android:layout_height="42dp"
        android:layout_marginBottom="50dp"
        android:layout_marginEnd="24dp"
        android:layout_marginStart="24dp"
        app:layout_constraintBottom_toTopOf="@+id/attend_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/attend_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="atttend"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/name_label_student"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:text="Name:"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/national_id_label"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_value"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="24dp"
        android:textColor="#000"
        android:layout_marginTop="8dp"
        android:text="TextViewTextView"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/national_id_value"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/department_label"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="24dp"
        android:textSize="17sp"
        android:text="Dep.:"
        app:layout_constraintBottom_toTopOf="@+id/sessions_spinner"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/department_value"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="24dp"
        android:textSize="17sp"
        android:textColor="#000"
        android:text="TextViewTextView"
        app:layout_constraintBottom_toTopOf="@+id/sessions_spinner"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/level_label"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="32dp"
        android:textSize="17sp"
        android:layout_marginStart="24dp"
        android:text="Level:"
        app:layout_constraintBottom_toTopOf="@+id/department_label"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/level_value"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="24dp"
        android:textSize="17sp"
        android:textColor="#000"
        android:text="TextViewTextView"
        app:layout_constraintBottom_toTopOf="@+id/department_value"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/national_id_label"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="24dp"
        android:text="NationalID:"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/seating_number_label"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/national_id_value"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginBottom="32dp"
        android:textColor="#000"
        android:layout_marginEnd="24dp"
        android:textSize="17sp"
        android:text="TextViewTextViewTextView"
        app:layout_constraintBottom_toTopOf="@+id/seating_number_value"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/seating_number_label"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="24dp"
        android:text="SeatingID:"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/level_label"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/seating_number_value"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="24dp"
        android:textColor="#000"
        android:textSize="17sp"
        android:text="TextViewTextViewTextView"
        app:layout_constraintBottom_toTopOf="@+id/level_value"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>
